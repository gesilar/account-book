{
  "name": "io.cordova.hellocordova",
  "displayName": "HelloCordova",
  "version": "1.0.1",
  "description": "A sample Apache Cordova application that responds to the deviceready event.",
  "main": "index.js",
  "scripts": {
    "bootstrap": "cd ./account-book-ui && npm i",
    "build-ui": "cd ./account-book-ui && npm run build",
    "build": "cd ./account-book-ui && npm run build && cordova build android",
    "emulate": "cd ./account-book-ui && npm run build && cordova emulate android",
    "web": "cd ./account-book-ui && npm run start"
  },
  "keywords": [
    "ecosystem:cordova"
  ],
  "author": "Apache Cordova Team",
  "license": "Apache-2.0",
  "devDependencies": {
    "cordova-plugin-camera": "^5.0.1",
    "cordova-plugin-file": "^6.0.2",
    "cordova-plugin-whitelist": "^1.3.4",
    "cordova-plugin-x-toast": "^2.7.2"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-camera": {
        "ANDROID_SUPPORT_V4_VERSION": "27.+"
      },
      "cordova-plugin-file": {},
      "cordova-plugin-x-toast": {}
    },
    "platforms": [
      "android"
    ]
  },
  "dependencies": {}
}